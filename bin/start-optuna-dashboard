#!/bin/bash
# -----------------------------------------------------------------------------
# Functions
# -----------------------------------------------------------------------------
function current_path() {
    echo $(cd $(dirname $0); pwd)
}

function activate() {
    eval "$(conda shell.bash hook)"
    conda activate $1
}
# -----------------------------------------------------------------------------
#
#
#
# -----------------------------------------------------------------------------
# Main
# -----------------------------------------------------------------------------
source "$(current_path)/../config.conf"
activate $ENV

nohup optuna-dashboard $DB_URL --port $PORT --host $HOST &
# -----------------------------------------------------------------------------
