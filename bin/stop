#!/bin/bash
# -----------------------------------------------------------------------------
# Functions
# -----------------------------------------------------------------------------
function get_pit() {
    ps -fea | grep -v grep | grep $1 | grep -v grep | awk '{print $2}' | head -n 1
}
# -----------------------------------------------------------------------------
#
#
#
# -----------------------------------------------------------------------------
# Main
# -----------------------------------------------------------------------------
kill -9 $(get_pit "optuna-dashboard")
# -----------------------------------------------------------------------------
