#!/bin/zsh
# -----------------------------------------------------------------------------
# Main
# -----------------------------------------------------------------------------
ABSPATH=$(readlink -f $0)
ABSDIR=$(dirname $ABSPATH)

source "$ABSDIR/../config.conf"
source "$CONDA_PATH/etc/profile.d/conda.sh"

echo "ENV: $ENV"
conda activate $ENV

echo "Start server..."
/usr/bin/nohup optuna-dashboard $DB_URL --port $PORT --host $HOST &
# -----------------------------------------------------------------------------

